package genelic;

class Box {
	private Object obj;

	public Object getObj() {
		return obj;
	}

	public void setObj(Object obj) {
		this.obj = obj;
	}

}

public class Ex02 {

	public static void main(String[] args) {
		Box box = new Box();

		box.setObj("오늘 좋은 하루되세요");
		
		String str =(String)box.getObj();	//반환타입이 obj라 다운캐스팅 필요
		
	}

}
