package condition;

import java.util.Scanner;

public class Ex03 {

	public static void main(String[] args) {

		// if else 문을 사용해서 2개의 정수를 입력 받아 해당 변수에 저장 후
		// 몇 번째 입력 받은 정수가 큰 수인지 확인

		Scanner sc = new Scanner(System.in);
		
		System.out.println("첫 번째 정수를 입력해주세요:  ");
		int a = sc.nextInt();
		
		System.out.println("두 번째 정수를 입력해주세요:  ");
		int b = sc.nextInt();

		if (a > b) {
			System.out.println("첫번째 수가 더 큽니다.");
		} else if(a < b) {
			System.out.println("두번째 입력한 수가 더큽니다.");
		}
		sc.close();
	}

}
