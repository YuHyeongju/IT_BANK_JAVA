	String name = "홍길동";
	
	public void study() {
		// 지역변수
		String place = "도서관";
		int time = 2;
		System.out.println(place+ "에서"+time+"시간동안 공부합니다.");
	}
	
	public void profile() {
		System.out.println();
		System.out.println();
	}
	
	
	
	
}




