package modifier;

class Box{
	static int boxID = 0; //static 맴버변수 :메모리상에 정적이다.
						  //프로그램 시작시 메모리의 메소드[ 클래스, static] 영역에 할당
						  //공유되는 전역 변수로 사용
	
	int idNum = 0;
	//인스턴스변수: 객체 생성시 객체가 가지고있는 맴버
	//객체 생성시 힙 메모리에 할당됨
	
    public void increase() {
    	boxID++;
    	idNum++;
    	
    }
}


public class Ex01 {

	public static void main(String[] args) {

	}

}
