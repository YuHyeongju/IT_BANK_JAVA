package inner;

class Outer5{
	
	public void aaa() {
		System.out.println("aaa");
	}
	public void bbb() {
		System.out.println("bbb");
	}
	public void ccc() {
		System.out.println("ccc");
	}
}




public class Ex05 {

	public static void main(String[] args) {
		Outer5  O5 = new Outer5() { // 익명클래스 Outer5의 메소드를 가져와서 수정할수 있음
			
			
			
		}; //세미콜론
		
		O5.aaa();
		O5.bbb();
		O5.ccc();
	}

}
