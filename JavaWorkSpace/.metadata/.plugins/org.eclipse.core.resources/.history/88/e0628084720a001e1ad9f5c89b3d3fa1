package loop;

public class Ex09 {

	public static void main(String[] args) {
		
		//이중 for문과 System.out.print("*")사용하여 
		//*****
		//*****
		//*****
		//*****
		//*****
		// 이런 모양만들기
		
		for(int i = 1; i <=5; i++) {
			System.out.print("*");
			for(int j=1;  j<=5; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		System.out.println("=================");
		
		for(int i = 0; i <=5; i++) {
			System.out.print("*");
			for(int j=1;  j<=i; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		
	}

}
