	@Override
	public void memberList() {
		//전체 회원 목록 출력(이름,나이, 전화번호)
		for(int i = 0; i < list.size(); i++) {
			System.out.println("이름: " + list.get(i).getName());
			System.out.println("나이: " + list.get(i).getAge()+"살");
			System.out.println("전화번호: " + list.get(i).getTel());
		}
		
	}

