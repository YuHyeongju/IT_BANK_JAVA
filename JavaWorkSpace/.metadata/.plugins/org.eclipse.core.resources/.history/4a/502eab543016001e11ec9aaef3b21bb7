package api;


class Person{
	 private String name;
	 private int age;
	 private final String SSN;
	 
	 public Person (String name, int age, String ssn) {
		 this.name = name;
		 this.age = age;
		 this.SSN = ssn;
	 }
	 
	 
	 
	 
	 
}






public class Ex01 {

	public static void main(String[] args) {
		Person person1 = new Person("홍길동", 20, "123456-1234567");
		Person person2 = new Person("이순신", 20, "123456-1234567");
	}

}
