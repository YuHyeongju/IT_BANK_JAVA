package inheritance;


class Sedan{
	
	public String color;
	
	public void Sedan() {
		System.out.println("Sedan 클래스 생성자 입니다.");
	}
}

class Sonata extends Sedan{
	
	public Sonata() {
		super(); //부모생성자 호출
		System.out.println("Sonata 클래스 생성자 입니다.");
	}
}


public class Ex02 {

	public static void main(String[] args) {
		
		Sonata sonata = new Sonata();
	}

}
