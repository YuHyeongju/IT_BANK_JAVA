package varialble;

public class Ex09 {
	
	
	
	public static void main(String[] args) {
		
		//이름, 전화번호, 주소를 String타입의 변수에 저장하고 출력하기
		String name ="유형주";
		String pn = "010-8853-2817";
		String ad = "부산시 금정구 두실로68 4층";
		
		System.out.println(name);
		System.out.println(pn);
		System.out.println(ad);
	}
}
