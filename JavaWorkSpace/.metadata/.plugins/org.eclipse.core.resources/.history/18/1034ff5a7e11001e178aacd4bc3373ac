package inner;


class Outer3{
	private int x = 100;
	private static int y = 200;
	
	static class Inner3{ //정적멤버 클래스
		
		private int z = 300;
		private static int zz = 400;
		
		public void disp() {
			//System.out.println("Outer x: " + x);
			//정적 멤버 클래스에서 바깥 멤버 클래스의 인스턴스 멤버는 사용 불가
			//사용하고싶다면 바깥클래스에서 객체를 생성하던가 x를 static으로 만든다.
			
			System.out.println("Outer y : " + y );
			System.out.println("Inner z : " + z );
			System.out.println("Inner zz : " + zz );
		}
	}
}



public class Ex03 {

	public static void main(String[] args) {

	}

}
