package varialble;

public class Ex02 {
	public static void main(String[] args) {
		//변수명 
		int price1 = 100;
		int $price = 200;
		int _price = 300;
		
		//$ _ 외 다른 특수기호 사용불가
		//숫자가 먼저 올 수 없음
		
		
		System.out.println(price1);
		System.out.println($price);
		System.out.println(_price);
		
		String firstname = "java";
		String firstName = "자바";
		
		//문자열을 저장하는 타입 
		//자바의 변수는 대소문자를 구분한다.
		
		System.out.println(firstname);
		System.out.println(firstName);
		
	}
}
