package abstraction;


abstract class Animal{//추상클래스
	
	public abstract void sound(); 
		//추상메소드, 실제 구현은 자식메소드에서 구현 
	
	
	
}

class Dog extends Animal{
	
	
	//자식클래스에서 부모의 추상메소드는 강제 오버라이딩
	
	@Override
	public void sound() {
		System.out.println("멍멍");
	}
}




public class Ex01 {

	public static void main(String[] args) {

	}

}
