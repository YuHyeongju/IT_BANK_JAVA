package class1;

class Person{// 객체를 생성시키는 설계도
	//클래스 구성맴버
	//필드,생성자,메소드
	
	
	//필드(객체의 속성)
	//class안에 선언된 변수
	
	//생성자 (객체초기화)
	//생성자는 클래스이름과 같은 이름
	public Person() {}
	
	//메소드(객체의 기능)
	public void eat() {}
	public void run() {}
	
}


public class ClassMain {
	
	public static void main(String[] args) {
		
		Person object = new Person();
		//클래스 타입 클래스변수 = new 생성자
		
		
		
		
	}

}
