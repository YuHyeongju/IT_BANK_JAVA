package exception;

public class Ex01 {

	public static void main(String[] args) {
		
		//system.out.println(); 컴파일 오류 
		//자바컴파일러가 구문체크를 해줌
		
		//System.out.println(args[0]);
		//실행에러 : 실행중 오류발생
	
		try {
			//예외가 발생 할 가능성이 있는 코드 넣기
			System.out.println(args[0]);
			System.out.println("Hello World");
		}catch(Exception e){
			//예외처리를 위한 코드를 넣는다.
			System.out.println("예외가 발생되었습니다.");
		}finally {
			//예외발생 여부에 상관없이 항상 수행되어야되는코드 넣기
			
			System.out.println("항상 실행됩니다.");
		}
	
	
	
	
	}
	
	

}
