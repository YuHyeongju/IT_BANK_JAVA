package array;

import java.util.Scanner;

public class Ex08 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int nums[] = new int[5];
		
		for(int i = 0; i <5; i++) {
			System.out.println(i+1+"번째 정수 입력: ");
			nums[i] = sc.nextInt();
		}
		
		int temp = 0;
		
		//이중 for문을 사용하여 nums배열 안의 5개의 숫자를
		//index 0 부터 작은 수 로 만들기 
		
		
		
		
	}

}
