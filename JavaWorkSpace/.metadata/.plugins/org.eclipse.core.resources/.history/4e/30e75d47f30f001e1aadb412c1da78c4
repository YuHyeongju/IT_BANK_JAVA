package class1.array;

class Student{
	
	private String name;
	private int age;
	
	public Student(String name, int age ) {
		this.name = name;
		this.age = age;
	}

	public String getName() {
		return name;
	}

	
	public int getAge() {
		return age;
	}

	
}




public class Ex02 {

	public static void main(String[] args) {

	}

}
