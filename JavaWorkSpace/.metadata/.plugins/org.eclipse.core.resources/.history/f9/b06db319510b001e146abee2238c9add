package array;

public class Ex04 {

	public static void main(String[] args) {

		int intArray[] = { 10, 20, 30, 40, 50, 60, 70 };

		// for문을 사용하여 배열의 각 요소 출력
		// 배열의 요소중에서 40값만 제외한 후에 나머지 요소출력

		for (int i = 0; i < intArray.length; i++) {
			
			
			/*
			 * if(i == 3){
			  	continue
			   }
			 
			 *if (intArray[i] == 40){
			 	continue; 
			  }
			*/
			
			
			
			if (i != 3) {
				
				
				
				System.out.println("intArray[" + i + "] : " + intArray[i]);
			}

		}

	}

}
