package operation;

import java.util.Scanner;

public class Ex05 {

	public static void main(String[] args) {
		
		int a,b;
		int result;
		
		Scanner sc = new Scanner(System.in);
		
		//두개의 정수를 입력받아 a,b 변수에 저장 후 사칙연산후 계산후 결과값 출력하기
		
		a = sc.nextInt();
		b = sc.nextInt();
		
		System.out.println("a와 b의  합은 : %d" + a + b);
		System.out.println("a와 b의  차는 : %d" + a - b);
		System.out.println("a와 b의  곱은 : %d" + a * b);
		System.out.println("a와 b의  나누기는 : %d" + a / b);
	}

}
