package random;

public class Ex02 {

	public static void main(String[] args) {
		
		int[] lotto = new int[6];
		
		//Math.random()을 사용하여 1 ~ 45 까지 수 중에 6를 뽑아 
		//lotto배열에 각 요소에 저장 후 출력
		
		for(int i = 0; i < 6; i++) {
			
			 lotto[i] = (int)(Math.random() * 45) +1;  //->중복 수 가 존재 할 수있음
			
		}
		for(int i = 0; i <6; i++) {
			System.out.println(lotto[i]+ " ");
		
		}
	}

}
