package input;

import java.util.Scanner;

public class Ex03 {

	public static void main(String[] args) {
		//Scanner를 사용하여 이름, 나이, 키 ,몸무게를 입력받고 해당변수에 저장후 출력
		Scanner sc = new Scanner(System.in);
		
		String name = sc.next();
		
		int age = sc.nextInt();
		
		double h = sc.nextDouble();
		
		double w = sc.nextDouble();
		
	}

}
