package array;

public class Ex03 {

	public static void main(String[] args) {
		
	 int intArray[] = new int[5];
	 
	 //intArray 배열의 각 요소에 인덱스를 사용하여 
	 //100,200,300,400,500의 정수값을 저장 후 출력하기
	 
	  intArray[0] = 100;
	  intArray[1] = 200;
	  intArray[2] = 300;
	  intArray[3] = 400;
	  intArray[4] = 500;
		
	}

}
