package loop;

public class Ex08 {

	public static void main(String[] args) {
		
		/*
		 * for(int i=0; i <10; i++) {
		 * 
		 * System.out.printf("2X%d=%d\n", i, 2*i);//구구단 2단
		 * 
		 * }
		 */
		
		//이중 for문을 사용해서 구구단 2단~9단 출력
		for(int i = 2; i<=9; i++) {
			
			for(int j = 1; j <=9; j++) {
				
				System.out.printf("%dx%d=%d\n",i,j,i*j);
			
			}
		}
		
	}

}
