package wrapper;







public class Ex01 {
	
	public static void main(String[] args) {
		//1.매개변수로 객체가 요구되는 경우
		//2. 기본타입이  아닌 객체로 저장해야하는경우 
		//3. 객체간의 비교가 필요한 경우 등에 wrapper 클래스가 쓰인다.
		//4. 객체간의 비교가 필요한 경우 등에 wrapper 클래스가 쓰인다.
		
		int a = 100; //기본타입 선언
		
		Integer obj = new Integer(a);// 기본타입의 값을 갖는 객체 생성(박싱)
		
		obj.intValue(); //포장된 객체에서 다시 기본타입의 값을 얻는다.(언박싱)
		// 기본타입의 이름 + Value();
	}
}
